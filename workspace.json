{
  "version": 1,
  "projects": {
    "lusobites-frontend": {
      "root": "lusobites/src",
      "sourceRoot": "lusobites/src",
      "projectType": "application",
      "targets": {
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "lusobites-frontend:build"
          }
        },
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/lusobites-frontend",
            "index": "lusobites/public/index.html",
            "main": "lusobites/src/index.js",
            "polyfills": "lusobites/src/polyfills.js",
            "tsConfig": "lusobites/tsconfig.app.json",
            "assets": ["lusobites/public"],
            "styles": ["lusobites/src/App.css"],
            "scripts": []
          }
        }
      }
    },
    "lusobites-backend": {
      "root": "lusobites/lusobites-backend",
      "sourceRoot": "lusobites/lusobites-backend",
      "projectType": "application",
      "targets": {
        "serve": {
          "executor": "nx:run-commands",
          "options": {
            "command": "node lusobites/lusobites-backend/server.js"
          }
        }
      }
    }
  }
}
