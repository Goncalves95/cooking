{
  "version": 1,
  "projects": {
    "lusobites": {
      "root": "apps/lusobites",
      "sourceRoot": "apps/lusobites/src",
      "projectType": "application",
      "targets": {
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "lusobites:build"
          }
        },
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/lusobites",
            "index": "apps/lusobites/public/index.html",
            "main": "apps/lusobites/src/index.js",
            "polyfills": "apps/lusobites/src/polyfills.js",
            "tsConfig": "apps/lusobites/tsconfig.app.json",
            "assets": ["apps/lusobites/public"],
            "styles": ["apps/lusobites/src/App.css"],
            "scripts": []
          }
        }
      }
    },
    "lusobites-backend": {
      "root": "lusobites-backend",
      "sourceRoot": "lusobites-backend",
      "projectType": "application",
      "targets": {
        "serve": {
          "executor": "nx:run-commands",
          "options": {
            "command": "node lusobites/lusobites-backend/server.js"
          }
        }
      }
    }
  }
}
